{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateName.module.scss",
        "index": "templates/default/index.tsx"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx",
        "style": "templates/page/TemplateName.scss",
        "lazy": "templates/page/TemplateName.lazy.tsx",
        "story": "templates/page/TemplateName.story.tsx",
        "test": "templates/pag/TemplateName.test.tsx"
      },
      "path": "src/pages",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    },
    "layout": {
      "path": "src/layouts",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    },
    "section": {
      "path": "src/sections",
      "withLazy": false,
      "withStory": false,
      "withStyle": false,
      "withTest": false
    },
    "provider": {
      "path": "src/providers",
      "withLazy": false,
      "withStory": true,
      "withStyle": false,
      "withTest": true,
      "withTypes": true,
      "withContext": true,
      "customDirectory": "TemplateNameProvider",
      "customTemplates": {
        "component": "templates/provider/TemplateName.tsx",
        "context": "templates/provider/TemplateName.context.ts",
        "story": "templates/provider/TemplateName.stories.tsx",
        "test": "templates/provider/TemplateName.test.tsx",
        "types": "templates/provider/TemplateName.types.ts"
      }
    }
  },
  "usesStyledComponents": true
}